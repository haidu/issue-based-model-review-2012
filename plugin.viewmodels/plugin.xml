<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="plugin.viewmodels.handlers.ViewModelsHandler"
            id="plugin.viewmodels.viewCommand"
            name="View Models">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.unicase.ui.navigator.viewer">
         <command
               commandId="plugin.viewmodels.viewCommand"
               label="Show Model View"
               style="push">
         </command>
         <command
               commandId="org.unicase.ui.common.commands.annotateIssue"
               icon="Icons/Issue.gif"
               label="Issue"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:plugin.viewmodels.modelview">
         <command
               commandId="org.unicase.ui.common.commands.annotateIssue"
               icon="Icons/Issue.gif"
               label="Issue"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="plugin.viewmodels.views.ModelView"
            id="plugin.viewmodels.modelview"
            name="Model View"
            restorable="true">
      </view>
   </extension>

</plugin>
